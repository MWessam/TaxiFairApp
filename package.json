    "build:aab:minor": "node ./scripts/build-aab.js minor",
    "build:aab:major": "node ./scripts/build-aab.js major",
    "build:aab:prebuild": "node ./scripts/build-aab.js patch --prebuild",
    "build:aab:clean": "node ./scripts/build-aab.js patch --force-clean",
    "build:aab:full": "node ./scripts/build-aab.js patch --prebuild --force-clean"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
